        <Label Name="alturaTEXT" Content="Label" HorizontalAlignment="Left" Margin="10,47,0,0" VerticalAlignment="Top" Height="32" Width="75"/>
        <Label Name="ventoTEXT" Content="Label" HorizontalAlignment="Left" Margin="10,84,0,0" VerticalAlignment="Top" Height="32" Width="75"/>
        <Label Name="anguloTEXT" Content="Label" HorizontalAlignment="Left" Margin="10,121,0,0" VerticalAlignment="Top" Height="32" Width="75"/>
        <Label Name="quebraTEXT" Content="Label" HorizontalAlignment="Left" Margin="10,158,0,0" VerticalAlignment="Top" Height="32" Width="75"/>
        <Label Name="terrenoTEXT" Content="Label" HorizontalAlignment="Left" Margin="10,195,0,0" VerticalAlignment="Top" Height="32" Width="75"/>
        <Label Name="processoTEXT" Content="Label" HorizontalAlignment="Center" VerticalAlignment="Top" Height="32" Width="75"/>
        <Label Name="mapaTEXT" Content="Label" HorizontalAlignment="Center" VerticalAlignment="Top" Height="32" Width="75" Margin="0,47,0,0"/>
    </Grid>
</Window>
